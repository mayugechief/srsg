<%

#fields = {} #local-variable

%><dl class="see">
  <dt><%= @model.lang :route %></dt>
  <dd><%= @item.label :route %></dd>
  
  <dt><%= @model.lang :name %></dt>
  <dd><%= @item.send :name %></dd>
  
  <% if fields[:filename] != -1 %>
  <dt><%= @model.lang :basename %></dt>
  <dd><%= @item.send :basename %></dd>
  <% end %>
  
  <dt><%= @model.lang :state %></dt>
  <dd><%= @item.label :state %></dd>
  
  <% if fields[:layout_id] != -1 %>
  <dt><%= @model.lang :layout_id %></dt>
  <dd><%= tryb { @item.layout.name }%></dd>
  <% end %>
  
  <% if fields[:shortcut] != -1 %>
  <dt><%= @model.lang :shortcut %></dt>
  <dd><%= @item.send :shortcut %></dd>
  <% end %>
</dl>
